<!DOCTYPE html>
<html>
<head>
  <title>Магазин</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='wrapper'>
    <div id='header'>
       <div id='page-title'>
          <h1><%= link_to '<strong>Магазин</strong>'.html_safe, root_path %></h1>
        </div>
        <%= render "./layouts/menu" %> 
    </div>

    <div id='middle'>
      <div id='container'>
        <div id='content'>      
        <p id="notice"><%= notice %></p>
        <%= yield %>
        </div>
      </div>
    </div>           
  </div>
<% debug params if Rails.env.development? %>
<% debug session[:user_id] if Rails.env.development? %>
<% debug current_cart if Rails.env.development? %>
<% debug current_user if Rails.env.development? %>
</body>
</html>