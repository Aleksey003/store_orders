<!DOCTYPE html>
<html>
<head>
  <title>StoreOrders</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id='wrapper'>
    <div id='header'>
       <div id='page-title'>
          <h1><%= link_to 'Shopping cart', root_path %></h1>
        </div>
        <%= render "./layouts/menu" %> 
    </div>
    <div id='content'>
      <div id='template'>
        <div id='customer'>      
        <p id="notice"><%= notice %></p>
        <%= yield %>
        </div>
      </div>
    </div>           
  </div>
<% debug params if Rails.env.development? %>
<% debug session[:user_id] if Rails.env.development? %>
</body>
</html>